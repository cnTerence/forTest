    guest : "",
    score : null,
    halfScore : null
  };
  
  this.host = {
    lgeRnk : 99,
    rcntMats : [],
    nextMats : []
  };
  
  this.guest = {
    lgeRnk : 99,
    rcntMats : [],
    nextMats : []
  };
  
  this.hstyBat = {
    mats : []
  };
  
  this.euroSp = {
    wl : [],
    lb : []
  };
  
  this.asiaSp = {
    am : [],
    wd : []
  };
  
  this.jcSp = [];
  
  this.jcSpR = [];
  
  this.euroKelly = "0-0-0";
  
  this.asiaKelly = "0-0-0";
  
  this.odrAmt = {
    bf : "0-0-0",
    jc : "0-0-0"
  };
  
  this.recmndn = "Y-Y-Y";
};

